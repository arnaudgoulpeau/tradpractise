{% extends 'layout.html.twig' %}

{% block content %}

    <table class="table table-hover">
        <thead>
            <tr>
                <th>{{ 'issue.name'|trans }}</th>
                <th>{{ 'issue.without.set'|trans }}</th>
                <th>{{ 'issue.without.partition'|trans }}</th>
                <th>{{ 'issue.without.abc'|trans }}</th>
            </tr>
        </thead>
        <tbody>
        {% for tune in tunes %}
            {% include ":tune:tune_detail_row.html.twig" with {tune: tune} %}
        {% endfor %}
        </tbody>
    </table>
{% endblock %}